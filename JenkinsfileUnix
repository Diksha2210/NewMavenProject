pipeline {
 agent any 
 stages {
  stage('build') {
     steps {
       withMaven(maven : maven-3.3.3.){
           sh "mvn clean compile"

          } 
       }
  }
  stage('Test') {
     steps {
       withMaven(maven : maven-3.3.3.){
           sh "mvn  test"
           }
         }
      }
       stage('deploy') {
     steps {
       withMaven(maven : maven-3.3.3.){
           sh "mvn  deploy"
           }
         }
      }
      
      }
  }
      
    
      
      
      
    
