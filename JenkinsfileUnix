pipeline { 
    agent any 
    stages {
        stage('Build') { 
        git url: 'https://github.com/Diksha2210/NewMavenProject.git'
            steps {
                withMaven{
                        sh "mvn clean compile"
                }
            }
        }
        stage('Test'){
            steps {
                withMaven{
                        sh "mvn test"
                }

            }
        }
        stage('Deploy') {
            steps {
               withMaven{
                        sh "mvn deploy"
                }

            }
        }
    }
}
